    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.14.0",
    "http-status": "^1.5.0",
    "jest": "^27.5.1",
    "knex": "^1.0.2",
    "knex-paginate": "^3.0.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.15",
